{
  "name": "projeto15",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --max-old-space-size=256 src/index.js",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* nodemon --inspect src/index.js",
    "build": "npm ci --only=production",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "lint": "echo 'Linting code...'",
    "test": "NODE_ENV=test npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "echo 'Running unit tests...'",
    "test:integration": "node scripts/test-whatsapp-integration.js",
    "migrate": "node scripts/migrate.js",
    "migrate:drop": "node scripts/migrate.js drop",
    "migrate:fresh": "npm run migrate:drop && npm run migrate",
    "generate-slots": "node -e \"require('./src/services/CronJobService').runManualSlotGeneration().then(() => console.log('Slots generated')).catch(console.error)\"",
    "backup": "node scripts/backup.js",
    "backup:create": "node scripts/backup.js create",
    "backup:list": "node scripts/backup.js list",
    "backup:info": "node scripts/backup.js info",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "tenant:init": "node scripts/init-multi-tenant.js",
    "tenant:create": "node scripts/provision-tenant.js create",
    "tenant:create-test": "node scripts/provision-tenant.js test",
    "tenant:multiple-test": "node scripts/provision-tenant.js multiple",
    "tenant:list": "node scripts/provision-tenant.js list",
    "tenant:cleanup": "node scripts/provision-tenant.js cleanup",
    "tenant:export": "node scripts/provision-tenant.js export",
    "tenant:import": "node scripts/provision-tenant.js import",
    "tenant:load-test": "node scripts/provision-tenant.js load-test",
    "tenant:report": "node scripts/provision-tenant.js report",
    "tenant:demo": "node scripts/demo-multi-tenant.js",
    "test-cron": "node scripts/test-cron.js",
    "test-chat": "node scripts/test-terminal-chat.js",
    "test-example": "node scripts/quick-test-example.js",
    "test-whatsapp": "node scripts/test-whatsapp-integration.js",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "logs": "tail -f logs/app.log",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "docker:build": "docker build -t agendamento-app .",
    "docker:run": "docker run -p 3000:3000 agendamento-app",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prod:setup": "npm run migrate && npm run generate-slots",
    "prod:health": "npm run health",
    "prod:backup": "npm run backup:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "nodemon": "^3.1.10"
  }
}
